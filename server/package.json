{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "start": "node dist/infra/http/server.mjs",
    "build": "tsup-node",
    "dev": "node scripts/start.js",
    "test": "node scripts/run-tests.js",
    "ci:test": "yarn db:migrate:test && dotenv -e .env.test -- vitest run",
    "lint:check": "biome check src",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:migrate:test": "dotenv -e .env.test -- drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "services:up": "docker compose -f docker/compose.yml up -d",
    "services:stop": "docker compose -f docker/compose.yml stop",
    "services:down": "docker compose -f docker/compose.yml down -v"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/node": "24.0.1",
    "dotenv-cli": "8.0.0",
    "drizzle-kit": "0.31.1",
    "tsup": "8.5.0",
    "tsx": "4.20.3",
    "typescript": "5.8.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.3"
  },
  "dependencies": {
    "@fastify/cors": "11.0.1",
    "@fastify/swagger": "9.5.1",
    "@fastify/swagger-ui": "5.2.3",
    "csv-stringify": "6.5.2",
    "date-fns": "4.1.0",
    "drizzle-orm": "0.44.2",
    "fastify": "5.4.0",
    "fastify-type-provider-zod": "5.0.1",
    "postgres": "3.4.7",
    "uuidv7": "1.0.2",
    "zod": "3.25.64"
  }
}
